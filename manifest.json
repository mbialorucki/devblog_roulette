{
  "manifest_version": 2,
  "name":"Devblog roulette",
  "version": "1.0",
  "description": "Display random blog from https://github.com/abdelhai/devblogs",

  "browser_action":{
    "default_icon": "icon.png"
  },
  "background":{
    "scripts": ["load.js"],
    "persistant": false
  },
  "permissions":[
    "tabs",
    "https://raw.githubusercontent.com/abdelhai/devblogs/master/dataset"
  ]
}
